{
  "name": "project",
  "description": "A barebones PHP app, which can easily be deployed to Heroku.",
  "image": "heroku/php",
  "repository": "https://github.com/discord3641/project.git",
  "keywords": ["php" ],
  "addons": [ "heroku-postgresql" ],
  "env": {
    "SECRET_KEY": {
      "description": "php application",
      "generator": "secret"
    }
  },
  "environments": {
    "test": {
      "scripts": {
        "test-setup": "php manage.py collectstatic --noinput",
        "test": "php manage.py test"
      }
    }
  }
}
